<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping package="entity">    
    
    <class name="OrderinfoItem" table="orderinfoitem"> 
    <id name="id" type="int" column="id"> 
        <generator class="native"/> 
    </id> 
    <property name="bookid" column="bookid" type="java.lang.Integer"/>
    <property name="booknum" column="booknum" type="java.lang.Integer"/>
    <property name="singlePrice" column="singleprice" type="java.lang.Double"/>
    <property name="totalPrice" column="totalprice" type="java.lang.Double"/>
    </class>
    
</hibernate-mapping>